<?xml version="1.0" encoding="utf-8"?>
<!--AnotherSc2 Hack - Settings file-->
<!--This file contains various settings for the software-->
<!--Please don't mess it up!-->
<AnotherSc2Hack>
  <!--Changes that affect the whole application and in the global scope-->
  <Global>
    <DataRefresh>100</DataRefresh>
    <DrawingRefresh>100</DrawingRefresh>
    <Language>English</Language>
    <ChangeSizeAndPosition>NumPad0</ChangeSizeAndPosition>
    <OnlyDrawWhenUnpaused>False</OnlyDrawWhenUnpaused>
    <DrawOnlyInForeground>False</DrawOnlyInForeground>
  </Global>
  <!--Overlay specific settings. Seperated for each overlay-->
  <Overlays>
    <OverlayResources>
      <Hotkey1>ControlKey</Hotkey1>
      <Hotkey2>Menu</Hotkey2>
      <Hotkey3>NumPad1</Hotkey3>
      <TogglePanel>/res</TogglePanel>
      <ChangePosition>/rcp</ChangePosition>
      <ChangeSize>/rcs</ChangeSize>
      <FontName>Century Gothic</FontName>
      <DrawBackground>True</DrawBackground>
      <Width>600</Width>
      <Height>50</Height>
      <Opacity>100</Opacity>
      <X>0</X>
      <Y>0</Y>
      <RemoveAi>False</RemoveAi>
      <RemoveAllie>False</RemoveAllie>
      <RemoveNeutral>False</RemoveNeutral>
      <RemoveClanTag>False</RemoveClanTag>
      <RemoveLocalplayer>False</RemoveLocalplayer>
    </OverlayResources>
    <OverlayIncome>
      <Hotkey1>ControlKey</Hotkey1>
      <Hotkey2>Menu</Hotkey2>
      <Hotkey3>NumPad2</Hotkey3>
      <TogglePanel>/inc</TogglePanel>
      <ChangePosition>/icp</ChangePosition>
      <ChangeSize>/ics</ChangeSize>
      <FontName>Century Gothic</FontName>
      <DrawBackground>True</DrawBackground>
      <Width>600</Width>
      <Height>50</Height>
      <Opacity>100</Opacity>
      <X>0</X>
      <Y>0</Y>
      <RemoveAi>False</RemoveAi>
      <RemoveAllie>False</RemoveAllie>
      <RemoveNeutral>False</RemoveNeutral>
      <RemoveClanTag>False</RemoveClanTag>
      <RemoveLocalplayer>False</RemoveLocalplayer>
    </OverlayIncome>
    <OverlayWorker>
      <Hotkey1>ControlKey</Hotkey1>
      <Hotkey2>Menu</Hotkey2>
      <Hotkey3>NumPad3</Hotkey3>
      <TogglePanel>/wor</TogglePanel>
      <ChangePosition>/wcp</ChangePosition>
      <ChangeSize>/wcs</ChangeSize>
      <X>0</X>
      <Y>0</Y>
      <Width>200</Width>
      <Height>50</Height>
      <FontName>Century Gothic</FontName>
    </OverlayWorker>
    <OverlayApm>
      <Hotkey1>ControlKey</Hotkey1>
      <Hotkey2>Menu</Hotkey2>
      <Hotkey3>NumPad7</Hotkey3>
      <TogglePanel>/apm</TogglePanel>
      <ChangePosition>/acp</ChangePosition>
      <ChangeSize>/acs</ChangeSize>
      <FontName>Century Gothic</FontName>
      <DrawBackground>True</DrawBackground>
      <Width>600</Width>
      <Height>50</Height>
      <Opacity>100</Opacity>
      <X>0</X>
      <Y>0</Y>
      <RemoveAi>False</RemoveAi>
      <RemoveAllie>False</RemoveAllie>
      <RemoveNeutral>False</RemoveNeutral>
      <RemoveClanTag>False</RemoveClanTag>
      <RemoveLocalplayer>False</RemoveLocalplayer>
    </OverlayApm>
    <OverlayArmy>
      <Hotkey1>ControlKey</Hotkey1>
      <Hotkey2>Menu</Hotkey2>
      <Hotkey3>NumPad8</Hotkey3>
      <TogglePanel>/arm</TogglePanel>
      <ChangePosition>/arcp</ChangePosition>
      <ChangeSize>/arcs</ChangeSize>
      <FontName>Century Gothic</FontName>
      <DrawBackground>True</DrawBackground>
      <Width>600</Width>
      <Height>50</Height>
      <Opacity>100</Opacity>
      <X>0</X>
      <Y>0</Y>
      <RemoveAi>False</RemoveAi>
      <RemoveAllie>False</RemoveAllie>
      <RemoveNeutral>False</RemoveNeutral>
      <RemoveClanTag>False</RemoveClanTag>
      <RemoveLocalplayer>False</RemoveLocalplayer>
    </OverlayArmy>
    <OverlayUnits>
      <RemoveProductionLine>False</RemoveProductionLine>
      <RemoveChronoboost>False</RemoveChronoboost>
      <RemoveSpellCounter>False</RemoveSpellCounter>
      <SplitBuildingsAndUnits>True</SplitBuildingsAndUnits>
      <ShowBuildings>True</ShowBuildings>
      <ShowUnits>True</ShowUnits>
      <UseTransparentImages>True</UseTransparentImages>
      <PictureSize>45</PictureSize>
      <Hotkey1>ControlKey</Hotkey1>
      <Hotkey2>Menu</Hotkey2>
      <Hotkey3>NumPad9</Hotkey3>
      <TogglePanel>/uni</TogglePanel>
      <ChangePosition>/ucp</ChangePosition>
      <ChangeSize>/ucs</ChangeSize>
      <FontName>Century Gothic</FontName>
      <DrawBackground>True</DrawBackground>
      <Width>300</Width>
      <Height>50</Height>
      <Opacity>100</Opacity>
      <X>0</X>
      <Y>0</Y>
      <RemoveAi>False</RemoveAi>
      <RemoveAllie>False</RemoveAllie>
      <RemoveNeutral>False</RemoveNeutral>
      <RemoveClanTag>False</RemoveClanTag>
      <RemoveLocalplayer>False</RemoveLocalplayer>
    </OverlayUnits>
    <OverlayProduction>
      <RemoveChronoboost>False</RemoveChronoboost>
      <SplitBuildingsAndUnits>True</SplitBuildingsAndUnits>
      <ShowBuildings>True</ShowBuildings>
      <ShowUnits>True</ShowUnits>
      <ShowUpgrades>True</ShowUpgrades>
      <UseTransparentImages>True</UseTransparentImages>
      <PictureSize>45</PictureSize>
      <Hotkey1>ControlKey</Hotkey1>
      <Hotkey2>Menu</Hotkey2>
      <Hotkey3>NumPad4</Hotkey3>
      <TogglePanel>/pro</TogglePanel>
      <ChangePosition>/pcp</ChangePosition>
      <ChangeSize>/pcs</ChangeSize>
      <FontName>Century Gothic</FontName>
      <DrawBackground>True</DrawBackground>
      <Width>300</Width>
      <Height>50</Height>
      <Opacity>100</Opacity>
      <X>0</X>
      <Y>0</Y>
      <RemoveAi>False</RemoveAi>
      <RemoveAllie>False</RemoveAllie>
      <RemoveNeutral>False</RemoveNeutral>
      <RemoveClanTag>False</RemoveClanTag>
      <RemoveLocalplayer>False</RemoveLocalplayer>
    </OverlayProduction>
    <OverlayMaphack>
      <Hotkey1>ControlKey</Hotkey1>
      <Hotkey2>Menu</Hotkey2>
      <Hotkey3>NumPad5</Hotkey3>
      <TogglePanel>/map</TogglePanel>
      <ChangePosition>/mcp</ChangePosition>
      <ChangeSize>/mcs</ChangeSize>
      <X>0</X>
      <Y>0</Y>
      <Width>261</Width>
      <Height>255</Height>
      <RemoveAi>False</RemoveAi>
      <RemoveAllie>False</RemoveAllie>
      <RemoveNeutral>False</RemoveNeutral>
      <RemoveLocalplayer>False</RemoveLocalplayer>
      <RemoveDestinationLine>False</RemoveDestinationLine>
      <DestinationLine>Color [Yellow]</DestinationLine>
      <ColorDefensifeStructures>True</ColorDefensifeStructures>
      <RemoveVisionArea>False</RemoveVisionArea>
      <RemoveCamera>False</RemoveCamera>
    </OverlayMaphack>
  </Overlays>
</AnotherSc2Hack>